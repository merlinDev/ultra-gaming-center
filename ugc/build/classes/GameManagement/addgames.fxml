            <Font name="System Bold" size="13.0" />
         </font>
      </JFXCheckBox>
      <JFXCheckBox fx:id="play" checkedColor="#4d74f5" layoutX="587.0" layoutY="83.0" onAction="#playonlyselected" text="Play" textFill="WHITE" unCheckedColor="#4bfa84">
