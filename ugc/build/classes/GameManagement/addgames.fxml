<AnchorPane id="AnchorPane" prefHeight="466.0" prefWidth="755.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameManagement.AddnewGame2Controller">
   <children>
      <JFXTextField fx:id="gameid" labelFloat="true" layoutX="14.0" layoutY="34.0" onMouseClicked="#image3" prefHeight="25.0" prefWidth="160.0" promptText="Game ID:" style="-fx-text-fill: white;">
