         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="image" labelFloat="true" layoutX="216.0" layoutY="189.0" onKeyTyped="#imageView" onMouseClicked="#image" prefHeight="28.0" prefWidth="473.0" promptText="Image" style="-fx-text-fill: white;">
